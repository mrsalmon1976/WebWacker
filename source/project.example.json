{
  "name": "example",
  "baseUrl": "https://test.yoursite.co.za",
  "threadCount": 25,
  "sequentialEndpoints": false,
  "variables": {
    "username": "yourusername",
    "password": "yourpassword"
  },
  "authenticationEndpoint": {
    "path": "/token",
    "method": "POST",
    "body": "Username={{username}}&password={{password}}&grant_type=password",
    "responseAccessTokenJsonPath": "$.access_token"
  },
  "endpoints": [
    {
      "path": "/v2/example/search?searchTerm=test",
      "method": "GET",
      "hitCount": 1000,
      "isAuthenticated": true
    },
    {
      "path": "/v1/example/something",
      "method": "GET",
      "hitCount": 1000,
      "isAuthenticated": true
    }
  ]
}


